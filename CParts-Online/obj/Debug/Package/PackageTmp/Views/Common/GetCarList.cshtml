@model IEnumerable<DAO.CarsInfo>  
@{
    var carBrand = Model.GroupBy(m => m.Brand).ToList();
    
System.Web.Script.Serialization.JavaScriptSerializer jss = new System.Web.Script.Serialization.JavaScriptSerializer(); 
}
<div class="searchTitle">车型选择</div>
  <div class="form-group serchDiv">
      <select class=" serchSelect" id="nav_proBrand">  
          <option value="-1">选择汽车品牌</option>      
          @foreach (var item in carBrand)
          {
              <option value="@item.Key" >@item.Key</option>
          }
      </select>
  </div>
<div class="form-group serchDiv">
    <select class=" serchSelect" id="nav_proModel">
        <option value="-1">选择汽车型号</option>
    </select>
</div>
<div class="form-group serchDiv">
    <select class=" serchSelect" id="nav_proYear">
        <option value="-1">选择出产年份</option>
       
    </select>
</div>

<div class="searchTitle">产品类型</div>
<div class="form-group serchDiv">
    <button class="btn btn-default btn-block" id="btnSearchCar">选择类型</button>
</div>

    <script>
        var CarInfoList = '@jss.Serialize(Model)';
        var CarObjs = JSON2Obj(strZY(CarInfoList));
        
    </script>
    
    