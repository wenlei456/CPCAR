<script type="text/javascript">
    window.onload = function () {
        var args = document.location.toString();
        var lo = args.indexOf("?");
        if (parseInt(lo) > 1) {
            var agu = args.substr(lo + 1, 5)
            if (document.getElementById(agu) != null) {
                document.getElementById(agu).onclick();
                $("#" + agu).parent().slideDown("slow");
            }
        }
    }
</script>