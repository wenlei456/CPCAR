@model IEnumerable<ProductModule.model.CrowdFundingDeail>
@{
    ViewBag.Title = "CrowdFunding";
}
<!DOCTYPE html>
<!-- saved from url=(0061)http://z.m.jd.com/redoundCategoryProject.html?categoryId=yyzx -->
<html lang="en"><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="viewport" content="width=device-width">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

<title>众筹移动版</title>
<meta name="viewport" content="width=device-width,initial-scale=1.0,user-scalable=0,minimum-scale=1.0,maximum-scale=1.0,minimal-ui">
<meta name="keywords" content="keywords">
<meta name="format-detection" content="telephone=no">
<meta name="description" content="description">
<link rel="stylesheet" type="text/css" href="/list_file/base.css">
<link rel="stylesheet" type="text/css" href="/list_file/animate.css">
<link rel="stylesheet" type="text/css" href="/list_file/item_list.css">

	  </head>
         
<body data-sn-index="0" class="loaded">

<input type="hidden" name="orderfrom" id="orderfrom" value="">
<input type="hidden" name="praiseSwitchFlag" id="praiseSwitchFlag" value="open">


<div class="wapper_big_class" style="height: 2385px;">
<div class="big_class_drag " style="transform: translate3d(0px, 0px, 0px); height: 2385px;">
	<!-- 全部档位分类 -->
<div class="big_class load curr_class" data-index="0" id="quanbu" catename="全部" style="left: 0px;">
  <div class="item_head">
	<img src="/list_file/yyzx.jpg" class="item_img">
  </div>
  <div class="cardcon" style="height: 2248px;">
        	 <div id="quanbu_yyzx_card" class="page" projectpage="1" isloadpage="1" isloaded="true" visible="true">
	
    <!-- 档位分类项目App-->
      <div id="quanbu_yyzx_z_card" class="z_card">
      	        <!--card list-->
         
          @if (Model != null && Model.Count()>0)
          {
              foreach (var item in Model)
              {  
               <div class="card">
		            <div class="card_top">
					
		                <div class="top_title">
														@item.ListTille
		                </div>
		            </div>
		            <div class="card_con">
		                		                <a href="/Product/CrowdFindingDeail?cid=@item.id"><img src="http://www.91cpcar.com/@item.Banner" class="loaded"></a>

		            </div>
		            <div class="card_foot">
		        
							            <div class="card_foot">
		                <div class="jine">
		                
		                
									筹集金额<em>￥</em><span class="gotham" id="zcje">@(item.Price*(item.Number - item.StockNUM))</span>元/<em>￥</em><span class="gotham"> @(item.Price*item.Number) </span>元
						                </div>
						                <ul>
						                    <li>支持人数<br><span class="gotham"> @(item.Number - item.StockNUM)</span>人</li>
                                            @if (item.StockNUM==0)
                                            {
                                                <li>项目进度<br><span class="gotham">0%</span></li>
                                            }else
                                            {
						                    <li>项目进度<br><span class="gotham">@(item.Number/item.StockNUM*100)%</span></li>
                                            }
								                                        <li>剩余天数<br><span>@item.EndTime.Subtract(DateTime.Now).Days</span>天</li></ul>
                                    										                            <div class="progress_container"><div class="progress_bar tip" progress="100" style="width: 100%;"></div></div>
		                        		                    
		                <div class="exp_text"><a data-maxlen="36">@item.ListTille</a></div>
		            </div>
					
		        </div>
                    </div>   
              }
          }
		        

    <!-- 档位分类项目App end-->
    </div>
   	  </div>

</div>
<!-- 全部档位分类 end -->
</div>
</div>

<!-- /加载更多左右 -->
<!-- debug -->
<div class="debug">
  <p></p>
</div>
<!-- /debug -->
<div class="load_more">加载更多...</div>
</body></html>